<% if @users.blank? %>
<p>There are not any users currently in the system</p>
<% else %>
<p>These are the current users in our system</p>

<ul id='users'>
  <% @users.each do |c| %>
  <li>
    <p><%= c.email %>: <%= c.last_name %>, <%= c.first_name %></p>
  </li>
  <% end %>
</ul>

<% end %>
<p><%= link_to 'Register', {action: 'new'} %></p>
